{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "aws/aws-sdk-php": "~3.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.2",
        "friendsofcat/laravel-feature-flag": "^3.1.7",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.6",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "pfizer/cognito-auth-php": "^5.0",
        "pfizer/qa-automation": "^1.0",
        "pfizer/security-audit-laravel": "^2.1.0",
        "pusher/pusher-php-server": "^4.0",
        "ramsey/uuid": "^3.4",
        "spatie/laravel-permission": "^3.17"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.27",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.9",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.6",
        "pfizer/cat-quality-script": "^1.0@dev",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pfizer/qa-automation.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/pfizer/cognito-auth-php.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/pfizer/cat-quality-script.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/pfizer/security-audit-laravel.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/pfizer/support-widget"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 app",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 app",
        "check-metrics": "phpmetrics --report-json=report.json app && vendor/bin/quality_run_check.py report.json",
        "check-metrics-html": "phpmetrics --report-json=report.json --report-html=report app && open report/index.html",
        "check-phpstan": "php artisan code:analyse --paths=app --level=0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
